<div class="container">
<%= form_for @attendee, role: 'form' do |f| %>
  <div class "form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :home_town %>
    <%= f.text_field :home_town, class: 'form-control' %>
  </div>

    <%= f.submit 'Create Attendee' %>

    <% end %>
</div>